; Loops test
%%reg 8

@__setup_const
  ; The newline
  OBJMK 10 [true] [string] "\n"
RETRN

@main

  ; Create global const variables
  CALLF "__setup_const"

  OBJMK 100 [false] [int] 1
  OBJMK 101 [true] [int] 21

  ; Loop until 20 and exit
  @.loop

  PRINT 100
  PRINT 10

  IVADD 100 1
  
  ; Loop again if the counter object is not 20
  CXXEQ 100 101
  JMPNF ".loop"

RETRN
